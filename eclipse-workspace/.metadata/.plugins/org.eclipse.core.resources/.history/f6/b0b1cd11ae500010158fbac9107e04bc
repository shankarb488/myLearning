package javademo;

public class Loops4 {

	public static void main(String[] args) {
		
		int k=1;

		for (int i = 0; i <= 4; i++) {

			for (int j = 1; j <= i; j++) {
				
				System.out.print(j*3);
				k++;
				System.out.print(" ");
			}
			System.out.println("\t");
			
		}

	}

}

/*
 * 3 
 * 6 9 
 * 12 15 18
 */
